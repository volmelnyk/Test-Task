@model IEnumerable<string>

@using (Html.BeginForm("Index", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="col-lg-4 col-lg-offset-4 col-md-4 col-md-offset-4 col-sm-4 col-sm-offset-4">
        <div class="form-group">
            <label for="file" class="control-label">Виберіть файл:</label>
            <input type="file" name="file" id="file" class="form-control" />
        </div>
        <div class="form-group">
            <label for="file" class="control-label">@ViewBag.Message</label>
            <label for="file" class="control-label">Ключове слово для пошуку:</label>
            <input type="text" name="keyWord" id=" keyWord" class="form-control" />
        </div>
        <input type="submit" class="btn btn-success btn-block" value="Зберегти дані" />
    </div>

    <div>
        <table class="table">
            @foreach (var item in Model)
            {
                <tr>
                    <th>
                        @item
                    </th>
                </tr>
            }
    </table>
    </div>

}
